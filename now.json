{
  "version": 2,
  "name": "micro-sms",
  "scope": "telemark",
  "alias": [
    "sms.service.t-fk.no"
  ],
  "builds": [
    {
      "src": "/index.js",
      "use": "@now/node"
    },
    {
      "src": "/*.md",
      "use": "@now/md"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/README.html"
    },
    {
      "src": "/sms",
      "dest": "/index.js",
      "headers": {
        "Access-Control-Allow-Origin": "*",
        "Access-Control-Allow-Headers": "Origin, X-Requested-With, Content-Type, Accept, Authorization",
        "Access-Control-Allow-Methods": "POST, OPTIONS"
      }
    }
  ]
}